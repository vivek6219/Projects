
// Provide a default path to dwr.engine
if (dwr == null) var dwr = {};
if (dwr.engine == null) dwr.engine = {};
if (DWREngine == null) var DWREngine = dwr.engine;

if (ContentAlignmentDWRFacade == null) var ContentAlignmentDWRFacade = {};
ContentAlignmentDWRFacade._path = '/webapps/goal/csdwr';
ContentAlignmentDWRFacade.deleteAlignment = function(p0, p1, callback) {
  dwr.engine._execute(ContentAlignmentDWRFacade._path, 'ContentAlignmentDWRFacade', 'deleteAlignment', p0, p1, callback);
}
ContentAlignmentDWRFacade.createTemporaryContent = function(p0, p1, callback) {
  dwr.engine._execute(ContentAlignmentDWRFacade._path, 'ContentAlignmentDWRFacade', 'createTemporaryContent', p0, p1, callback);
}
ContentAlignmentDWRFacade.toggleAlignmentStudentVisibility = function(p0, p1, callback) {
  dwr.engine._execute(ContentAlignmentDWRFacade._path, 'ContentAlignmentDWRFacade', 'toggleAlignmentStudentVisibility', p0, p1, callback);
}
ContentAlignmentDWRFacade.initContextFromRequestHeader = function(callback) {
  dwr.engine._execute(ContentAlignmentDWRFacade._path, 'ContentAlignmentDWRFacade', 'initContextFromRequestHeader', false, callback);
}
